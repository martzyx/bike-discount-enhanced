{
  "manifest_version": 3,
  "name": "bike-discount enhanced",
  "version": "1.0",
  "description": "see name",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["*://*.bike-discount.de/*"],
      "js": ["ensure-dk.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "128": "icons/icon16.png",
  }
}
