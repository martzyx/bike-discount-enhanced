{
  "manifest_version": 3,
  "name": "bike-discount enhanced",
  "version": "0.1",
  "description": "see name",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["*://*.bike-discount.de/*"],
      "js": ["ensure-dk.js","login.js"]
    }
  ],
  "action": {
    "default_popup": "popup/popup.html"
  },
  "icons": {
    "128": "icons/icon128.png"
  }
}
